{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ESNext"],
    "types": ["node", "@cloudflare/workers-types"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "paths": {
      "@utils/*": ["./src/utils/*"],
      "@v1/*": ["./src/v1/*"]
    }
  },
  "include": ["worker-configuration.d.ts", "src/**/*.ts"]
}
